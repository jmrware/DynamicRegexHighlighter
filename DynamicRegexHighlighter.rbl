<?xml version="1.0" encoding="UTF-8"?>
<rxb:regexbuddy xmlns:rxb="http://www.regexbuddy.com/regexbuddy32.xsd" version="3.2">
	<libraryfile>
		<action id="Groupopening" dotall="1" freespacing="1" flavor="pcre" flavorreplace="jgsoft">
			<regex> # Regex to match a PCRE regex group opening sequence.&#13;&#10;\(                   # Opening parentheses delimiter.&#13;&#10;(                    # $1: Optional group type specification.&#13;&#10;  (?:                # Non-capture group to allow optional quantifier.&#13;&#10;    \?               # Special group types all start with a "?".&#13;&#10;    (?:              # Non-capture group for group type alternatives.&#13;&#10;      [:|>=!]        # Types specified with a single character.&#13;&#10;    | P?&lt;\w+>        # Named capturing group (?P&lt;Python> and (?&lt;Perl>.&#13;&#10;    | '\w+'          # Named capturing group (Perl).&#13;&#10;    | &lt;[=!]          # Look behind.&#13;&#10;    | \(             # Conditionals groups parentheses delimiter.&#13;&#10;      (?:            # Non-capture group for conditional alternatives.&#13;&#10;        [-+]?\d+     # Absolute/+-relative reference condition.&#13;&#10;      | &lt;\w+>        # Named reference condition (Perl).&#13;&#10;      | '\w+'        # Named reference condition (Perl).&#13;&#10;      | \w+          # Named reference condition (PCRE).&#13;&#10;      ) \)           # End conditional group type spec.&#13;&#10;    | (?:            # Groups that must have zero content.&#13;&#10;        R            # Recurse whole pattern.&#13;&#10;      | -?[iJmsUx]   # Flag modifiers.&#13;&#10;      | [-+]?\d+     # Call subpattern by absolute/+-relative number.&#13;&#10;      | &amp;\w+         # Call subpattern by name (Perl).&#13;&#10;      | P>\w+        # Call subpattern by name (Python).&#13;&#10;      | P=\w+        # Reference by name (Python).&#13;&#10;      )(?=\))        # Ensure this group type has no contents.&#13;&#10;    )                # End non-capture group of group type alternatives.&#13;&#10;  )?                 # Special group type is optional.&#13;&#10;)                    # End $n&#13;&#10;</regex>
			<description>Group opening</description>
			<testdata format="utf16le"></testdata>
		</action>
		<action id="Matchinnerparentheses" dotall="1" freespacing="1" flavor="pcre" flavorreplace="jgsoft">
			<regex> # Regex to match PCRE regex group.&#13;&#10;\(                   # Opening parentheses delimiter.&#13;&#10;(                    # $1: Optional group type specification.&#13;&#10;  (?:                # Non-capture group to allow optional quantifier.&#13;&#10;    \?               # Special group types all start with a "?".&#13;&#10;    (?:              # Non-capture group for group type alternatives.&#13;&#10;      [:|>=!]        # Types specified with a single character.&#13;&#10;    | P?&lt;\w+>        # Named capturing group (?P&lt;Python>) and (?&lt;Perl>).&#13;&#10;    | '\w+'        # Named capturing group (Perl).&#13;&#10;    | &lt;[=!]          # Look behind.&#13;&#10;    | \(             # Conditionals groups parentheses delimiter.&#13;&#10;      (?:            # Non-capture group for conditional alternatives.&#13;&#10;        [-+]?\d+     # Absolute/+-relative reference condition.&#13;&#10;      | &lt;\w+>        # Named reference condition (Perl).&#13;&#10;      | '\w+'      # Named reference condition (Perl).&#13;&#10;      | \w+          # Named reference condition (PCRE).&#13;&#10;      ) \)           # End conditional group type spec.&#13;&#10;    | (?:            # Groups that must have zero content.&#13;&#10;        R            # Recurse whole pattern.&#13;&#10;      | -?[iJmsUx]   # Flag modifiers.&#13;&#10;      | [-+]?\d+     # Call subpattern by absolute/+-relative number.&#13;&#10;      | &amp;\w+         # Call subpattern by name (Perl).&#13;&#10;      | P>\w+        # Call subpattern by name (Python).&#13;&#10;      | P=\w+        # Reference by name (Python).&#13;&#10;      )(?=\))        # Ensure this group type has no contents.&#13;&#10;    )                # End non-capture group of group type alternatives.&#13;&#10;  )?                 # Special group type is optional.&#13;&#10;)                    # End $1&#13;&#10;([^()\\]*+(?:\\.[^()\\]*+)*+)  # $2: inner contents&#13;&#10;\)                                   # closing delimiter&#13;&#10;((?:[?*+]|{\d+(?:,\d*)?})?+[+?]?+)   # $3 optional quantifier&#13;&#10;</regex>
			<description>Match inner parentheses</description>
			<testdata format="utf16le"></testdata>
		</action>
		<action id="PCRECharacterclass" dotall="1" freespacing="1" flavor="pcre" flavorreplace="jgsoft">
			<regex>  \[                                     # Character class opening delimiter.&#13;&#10;  (                                      # $1: Character class contents (cc).&#13;&#10;    [^\[\]\\]*(?:\\[\S\s][^\[\]\\]*)*    # Non-[], escaped-anything (normal*).&#13;&#10;    (?: \[                               # Allow a non-escaped "[", and it&#13;&#10;      (?::\^?\w+:\])?                    # may be embedded POSIX char class.&#13;&#10;      [^\[\]\\]*(?:\\[\S\s][^\[\]\\]*)*  # More non-[], escaped-anything.&#13;&#10;    )*                                   # Unroll-the-loop (special normal*)*&#13;&#10;  )                                      # End $1.&#13;&#10;  \]                                     # Character class closing delimiter.&#13;&#10;</regex>
			<description>PCRE Character class</description>
			<testdata format="utf16le"></testdata>
		</action>
		<action id="Quantifieroptional" dotall="1" freespacing="1" flavor="pcre" flavorreplace="jgsoft">
			<regex>((?:(?:[?*+]|{\d+(?:,\d*)?})[+?]?)?)  # $2: Optional quantifier.&#13;&#10;</regex>
			<description>Quantifier (optional)</description>
			<testdata format="utf16le"></testdata>
		</action>
		<action id="re_1_cmt" freespacing="1" flavor="pcre" flavorreplace="jgsoft">
			<regex># re_1_cmt: Match regex character classes, comments and comment groups.&#13;&#10;  (\[\^?)                                 # $1: Character class opening delim.&#13;&#10;  (                                       # $2: Character class contents.&#13;&#10;    \]?                                   # Unescaped ] allowed if first char.&#13;&#10;    [^\][\\]*(?:\\[\S\s][^\][\\]*)*       # Non-[], escaped-anything (normal*).&#13;&#10;    (?: \[                                # Allow a non-escaped "[", and it&#13;&#10;      (?::\^?\w+:\])?                     # may be embedded POSIX char class.&#13;&#10;      [^\][\\]*(?:\\[\S\s][^\][\\]*)*     # More non-[], escaped-anything.&#13;&#10;    )*                                    # Unroll-the-loop (special normal*)*&#13;&#10;  )                                       # End $2. Character class contents.&#13;&#10;  \]                                      # Character class closing delimiter.&#13;&#10;  ((?:(?:[?*+]|\{\d+(?:,\d*)?\})[+?]?)?)  # $3: Optional cc quantifier.&#13;&#10;| (\(\?\#[^)]*\))                         # $4: Comment group (cmt_grp).&#13;&#10;| (\((?!\?\#))                            # $5: Opening "(" (non comment group).&#13;&#10;| (\#.*)                                  # $6: Comment (cmt).&#13;&#10;| ([^[(#\\]*(?:\\[\S\s][^[(#\\]*)*)       # $7: Everything else.&#13;&#10;</regex>
			<description>re_1_cmt</description>
			<testdata format="utf16le"></testdata>
		</action>
		<action id="re_1_nocmt" freespacing="1" flavor="pcre" flavorreplace="jgsoft">
			<regex># re_1_nocmt: Match regex character classes and comment groups (no comments).&#13;&#10;  (\[\^?)                                 # $1: Character class opening delim.&#13;&#10;  (                                       # $2: Character class contents.&#13;&#10;    \]?                                   # Unescaped ] allowed if first char.&#13;&#10;    [^\][\\]*(?:\\[\S\s][^\][\\]*)*       # Non-[], escaped-anything (normal*).&#13;&#10;    (?: \[                                # Allow a non-escaped "[", and it&#13;&#10;      (?::\^?\w+:\])?                     # may be embedded POSIX char class.&#13;&#10;      [^\][\\]*(?:\\[\S\s][^\][\\]*)*     # More non-[], escaped-anything.&#13;&#10;    )*                                    # Unroll-the-loop (special normal*)*&#13;&#10;  )                                       # End $2. Character class contents.&#13;&#10;  \]                                      # Character class closing delimiter.&#13;&#10;  ((?:(?:[?*+]|\{\d+(?:,\d*)?\})[+?]?)?)  # $3: Optional cc quantifier.&#13;&#10;| (\(\?\#[^)]*\))                         # $4: Comment group (cmt_grp).&#13;&#10;| (\((?!\?\#))                            # $5: Opening "(" (non comment group).&#13;&#10;| ([^[(\\]*(?:\\[\S\s][^[(\\]*)*)         # $6: Everything else.&#13;&#10;</regex>
			<description>re_1_nocmt</description>
			<testdata format="utf16le"></testdata>
		</action>
		<action id="re_2" freespacing="1" flavor="pcre" flavorreplace="jgsoft">
			<regex># re_2: Match inner (non-nested) PCRE syntax regex groups.&#13;&#10;\(                         # Regex group opening "(" delimiter.&#13;&#10;(                          # $1: Optional group type specification.&#13;&#10;  \?                       # All special group types start with a "?".&#13;&#10;  (?:                      # Non-capture group for group types alternatives.&#13;&#10;    [:|>=!]                # Types specified with a single character.&#13;&#10;  | &amp;gt;                   # Atomic group (HTML entity).&#13;&#10;  | &amp;lt;[=!]               # Look behind (HTML entity).&#13;&#10;  | &lt;[=!]                  # Look behind (Note 1).&#13;&#10;  | P?&amp;lt;\w+&amp;gt;          # Named capture group (Python/Perl) (HTML entity).&#13;&#10;  | P?&lt;\w+>                # Named capture group (Python/Perl) (Note 1).&#13;&#10;  | '\w+'                  # Named capturing group (Perl).&#13;&#10;  | (?=&lt;span[^>]*>&amp;\#40;)  # Previously-marked nested generic conditional.&#13;&#10;  | \(                     # Begin conditional group with "(" delimiter.&#13;&#10;    (?:                    # Non-capture group for conditional alternatives.&#13;&#10;      [+\-]?\d+            # Absolute/+-relative reference condition.&#13;&#10;    | &amp;lt;\w+&amp;gt;          # Named reference condition (Perl) (HTML entity).&#13;&#10;    | &lt;\w+>                # Named reference condition (Perl) (Note 1).&#13;&#10;    | '\w+'                # Named reference condition (Perl).&#13;&#10;    | \w+                  # Named reference condition (PCRE)&#13;&#10;    | R&amp;amp;\w+            # specific recursion condition (HTML entity).&#13;&#10;    | R&amp;\w+                # specific recursion condition (Note 1).&#13;&#10;    ) \)                   # End conditional group with ")" delimiter.&#13;&#10;  | (?:                    # Group types that must have zero content.&#13;&#10;      R                    # Recurse whole pattern.&#13;&#10;    | (?:-?[iJmsUx])+      # Flag modifiers (PCRE).&#13;&#10;    | [+\-]?\d+            # Call subpattern by absolute/+-relative number.&#13;&#10;    | &amp;amp;\w+             # Call subpattern by name (Perl) (HTML entity).&#13;&#10;    | &amp;\w+                 # Call subpattern by name (Perl) (Note 1).&#13;&#10;    | P&amp;gt;\w+             # Call subpattern by name (Python) (HTML entity).&#13;&#10;    | P>\w+                # Call subpattern by name (Python) (Note 1).&#13;&#10;    | P=\w+                # Reference by name (Python).&#13;&#10;    )(?=\))                # Ensure this group type has no contents.&#13;&#10;  )                        # End non-capture group of group types alternatives.&#13;&#10;)?                         # End $1: Optional group type specification.&#13;&#10;([^()]*)                   # $2: Inner group contents.&#13;&#10;\)                         # Regex group closing ")" delimiter.&#13;&#10;((?:(?:[?*+]|\{\d+(?:,\d*)?\})[+?]?)?)  # $3: Optional quantifier.&#13;&#10;# Note 1: Handle "&lt;", ">" and "&amp;", even if not converted to HTML entities.&#13;&#10;</regex>
			<description>re_2</description>
			<testdata format="utf16le"></testdata>
		</action>
		<action id="re_escapedgroupdelims" freespacing="1" flavor="pcre" flavorreplace="jgsoft">
			<regex># re_escapedgroupdelims: Convert escaped group delim chars to HTML entities.&#13;&#10;(\\\()  |                    # $1: Escaped left parentheses, or...&#13;&#10;(\\\))  |                    # $2: Escaped right parentheses, or...&#13;&#10;(\\\|)  |                    # $3: Escaped OR pipe char, or...&#13;&#10;([^\\]*(?:\\[^()|][^\\]*)*)  # $4: Everything else.&#13;&#10;</regex>
			<description>re_escapedgroupdelims</description>
		</action>
	</libraryfile>
</rxb:regexbuddy>
